wait managerApi.updateTask(id, task);\n      const index = this.tasks.findIndex(t => t.id === id);\n      if (index !== -1) {\n        this.tasks.splice(index, 1, response.data);\n      }\n    } catch (error) {\n      this.error = error;\n    }\n  },\n\n  async deleteTask(id) {\n    try {\n      await managerApi.deleteTask(id);\n      this.tasks = this.tasks.filter(task => task.id !== id);\n    } catch (error) {\n      this.error = error;\n    }\n  }\n});\n"],"mappings":";;;AAAA;AACA,SAASA,QAAQ,QAAQ,KAAK;AAC9B,OAAOC,UAAU,MAAM,mBAAmB;AAE1C,OAAO,MAAMC,YAAY,GAAGF,QAAQ,CAAC;EACnCG,KAAK,EAAE,EAAE;EACTC,OAAO,EAAE,KAAK;EACdC,KAAK,EAAE,IAAI;EAEX,MAAMC,OAAOA,CAACC,IAAI,EAAE;IAClB,IAAI,CAACH,OAAO,GAAG,IAAI;IACnB,IAAI;MACFI,OAAO,CAACC,GAAG,CAAC,mBAAmB,EAAEF,IAAI,CAAC;MACtC,MAAMG,QAAQ,GAAG,MAAMT,UAAU,CAACU,cAAc,CAACJ,IAAI,CAAC;MACtD;MACAC,OAAO,CAACC,GAAG,CAAC,wBAAwB,GAAGG,IAAI,CAACC,SAAS,CAACH,QAAQ,CAAC,CAAC;MAChE,OAAOA,QAAQ,CAACI,IAAI;IACtB,CAAC,CAAC,OAAOT,KAAK,EAAE;MACdG,OAAO,CAACC,GAAG,CAAC,OAAO,EAAGJ,KAAK,CAAC;MAC5B,IAAI,CAACA,KAAK,GAAGA,KAAK;IACpB,CAAC,SAAS;MACR,IAAI,CAACD,OAAO,GAAG,KAAK;IACtB;EACF,CAAC;EAED,MAAMW,QAAQA,CAACC,IAAI,EAACC,MAAM,EAAE;IAC1B,IAAI;MACF,MAAMP,QAAQ,GAAG,MAAMT,UAAU,CAACc,QAAQ,CAACC,IAAI,EAACC,MAAM,CAAC;MACvD,IAAI,CAACd,KAAK,CAACe,IAAI,CAACR,QAAQ,CAACI,IAAI,CAAC;MAC9BN,OAAO,CAACC,GAAG,CAAC,eAAe,GAAGG,IAAI,CAACC,SAAS,CAACH,QAAQ,CAAC,CAAC;MACvD;MACA,OAAO,IAAI,CAACP,KAAK;IACnB,CAAC,CAAC,OAAOE,KAAK,EAAE;MACd,IAAI,CAACA,KAAK,GAAGA,KAAK;IACpB;EACF,CAAC;EAED,MAAMc,UAAUA,CAACC,EAAE,EAAEJ,IAAI,EAAE;IACzB,IAAI;MACF,MAAMN,QAAQ,GAAG,MAAMT,UAAU,CAACkB,UAAU,CAACC,EAAE,EAAEJ,IAAI,CAAC;MACtD,MAAMK,KAAK,GAAG,IAAI,CAAClB,KAAK,CAACmB,SAAS,CAACC,CAAC,IAAIA,CAAC,CAACH,EAAE,KAAKA,EAAE,CAAC;MACpD,IAAIC,KAAK,KAAK,CAAC,CAAC,EAAE;QAChB,IAAI,CAAClB,KAAK,CAACqB,MAAM,CAACH,KAAK,EAAE,CAAC,EAAEX,QAAQ,CAACI,IAAI,CAAC;MAC5C;IACF,CAAC,CAAC,OAAOT,KAAK,EAAE;MACd,IAAI,CAACA,KAAK,GAAGA,KAAK;IACpB;EACF,CAAC;EAED,MAAMoB,UAAUA,CAACL,EAAE,EAAE;IACnB,IAAI;MACF,MAAMnB,UAAU,CAACwB,UAAU,CAACL,EA