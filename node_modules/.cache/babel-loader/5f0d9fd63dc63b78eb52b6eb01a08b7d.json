N.stringify(this.tasks))\n      return this.tasks;\n    } catch (error) {\n      this.error = error;\n    }\n  },\n\n  async updateTask(id, task) {\n    try {\n      const response = await adminApi.updateTaskAdmin(id, task);\n      const index = this.tasks.findIndex(t => t.id === id);\n      if (index !== -1) {\n        this.tasks.splice(index, 1, response.data);\n      }\n    } catch (error) {\n      this.error = error;\n    }\n  },\n\n  async deleteTask(id) {\n    try {\n      await adminApi.deleteTaskAdmin(id);\n      this.tasks = this.tasks.filter(task => task.id !== id);\n    } catch (error) {\n      this.error = error;\n    }\n  }\n});\n"],"mappings":";;;AAAA;AACA,SAASA,QAAQ,QAAQ,KAAK;AAC9B,OAAOC,QAAQ,MAAM,iBAAiB;AAEtC,OAAO,MAAMC,UAAU,GAAGF,QAAQ,CAAC;EACjCG,KAAK,EAAE,EAAE;EACTC,OAAO,EAAE,KAAK;EACdC,KAAK,EAAE,IAAI;EAEX,MAAMC,OAAOA,CAACC,IAAI,EAAE;IAClB,IAAI,CAACH,OAAO,GAAG,IAAI;IACnB,IAAI;MACFI,OAAO,CAACC,GAAG,CAAC,mBAAmB,EAAEF,IAAI,CAAC;MACtC,MAAMG,QAAQ,GAAG,MAAMT,QAAQ,CAACU,YAAY,CAACJ,IAAI,CAAC;MAClD;MACAC,OAAO,CAACC,GAAG,CAAC,wBAAwB,GAAGG,IAAI,CAACC,SAAS,CAACH,QAAQ,CAAC,CAAC;MAChE,OAAOA,QAAQ,CAACI,IAAI;IACtB,CAAC,CAAC,OAAOT,KAAK,EAAE;MACdG,OAAO,CAACC,GAAG,CAAC,OAAO,EAAGJ,KAAK,CAAC;MAC5B,IAAI,CAACA,KAAK,GAAGA,KAAK;IACpB,CAAC,SAAS;MACR,IAAI,CAACD,OAAO,GAAG,KAAK;IACtB;EACF,CAAC;EAED,MAAMW,QAAQA,CAACC,IAAI,EAACC,MAAM,EAAE;IAC1B,IAAI;MACF,MAAMP,QAAQ,GAAG,MAAMT,QAAQ,CAACiB,aAAa,CAACF,IAAI,EAACC,MAAM,CAAC;MAC1D,IAAI,CAACd,KAAK,CAACgB,IAAI,CAACT,QAAQ,CAACI,IAAI,CAAC;MAC9BN,OAAO,CAACC,GAAG,CAAC,eAAe,GAAGG,IAAI,CAACC,SAAS,CAACH,QAAQ,CAAC,CAAC;MACvD;MACA,OAAO,IAAI,CAACP,KAAK;IACnB,CAAC,CAAC,OAAOE,KAAK,EAAE;MACd,IAAI,CAACA,KAAK,GAAGA,KAAK;IACpB;EACF,CAAC;EAED,MAAMe,UAAUA,CAACC,EAAE,EAAEL,IAAI,EAAE;IACzB,IAAI;MACF,MAAMN,QAAQ,GAAG,MAAMT,QAAQ,CAACqB,eAAe,CAACD,EAAE,EAAEL,IAAI,CAAC;MACzD,MAAMO,KAAK,GAAG,IAAI,CAACpB,KAAK,CAACqB,SAAS,CAACC,CAAC,IAAIA,CAAC,CAACJ,EAAE,KAAKA,EAAE,CAAC;MACpD,IAAIE,KAAK,KAAK,CAAC,CAAC,EAAE;QAChB,IAAI,CAACpB,KAAK,CAACuB,MAAM,CAACH,KAAK,EAAE,CAAC,EA