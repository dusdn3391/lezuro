ask(id);\n      this.tasks = this.tasks.filter(task => task.id !== id);\n    } catch (error) {\n      this.error = error;\n    }\n  }\n});\n"],"mappings":";AAAA;AACA,SAASA,QAAQ,QAAQ,KAAK;AAC9B,OAAOC,OAAO,MAAM,gBAAgB;AAEpC,OAAO,MAAMC,SAAS,GAAGF,QAAQ,CAAC;EAChCG,KAAK,EAAE,EAAE;EACTC,OAAO,EAAE,KAAK;EACdC,KAAK,EAAE,IAAI;EAEX,MAAMC,SAASA,CAAA,EAAG;IAChB,IAAI,CAACF,OAAO,GAAG,IAAI;IACnB,IAAI;MACF,MAAMG,QAAQ,GAAG,MAAMN,OAAO,CAACO,QAAQ,CAAC,CAAC;MACzC,IAAI,CAACL,KAAK,GAAGI,QAAQ,CAACE,IAAI;IAC5B,CAAC,CAAC,OAAOJ,KAAK,EAAE;MACd,IAAI,CAACA,KAAK,GAAGA,KAAK;IACpB,CAAC,SAAS;MACR,IAAI,CAACD,OAAO,GAAG,KAAK;IACtB;EACF,CAAC;EAED,MAAMM,OAAOA,CAACC,IAAI,EAAE;IAClB,IAAI;MACF,MAAMJ,QAAQ,GAAG,MAAMN,OAAO,CAACW,UAAU,CAACD,IAAI,CAAC;MAC/C,IAAI,CAACR,KAAK,CAACU,IAAI,CAACN,QAAQ,CAACE,IAAI,CAAC;IAChC,CAAC,CAAC,OAAOJ,KAAK,EAAE;MACd,IAAI,CAACA,KAAK,GAAGA,KAAK;IACpB;EACF,CAAC;EAED,MAAMS,UAAUA,CAACC,EAAE,EAAEJ,IAAI,EAAE;IACzB,IAAI;MACF,MAAMJ,QAAQ,GAAG,MAAMN,OAAO,CAACa,UAAU,CAACC,EAAE,EAAEJ,IAAI,CAAC;MACnD,MAAMK,KAAK,GAAG,IAAI,CAACb,KAAK,CAACc,SAAS,CAACC,CAAC,IAAIA,CAAC,CAACH,EAAE,KAAKA,EAAE,CAAC;MACpD,IAAIC,KAAK,KAAK,CAAC,CAAC,EAAE;QAChB,IAAI,CAACb,KAAK,CAACgB,MAAM,CAACH,KAAK,EAAE,CAAC,EAAET,QAAQ,CAACE,IAAI,CAAC;MAC5C;IACF,CAAC,CAAC,OAAOJ,KAAK,EAAE;MACd,IAAI,CAACA,KAAK,GAAGA,KAAK;IACpB;EACF,CAAC;EAED,MAAMe,UAAUA,CAACL,EAAE,EAAE;IACnB,IAAI;MACF,MAAMd,OAAO,CAACmB,UAAU,CAACL,EAAE,CAAC;MAC5B,IAAI,CAACZ,KAAK,GAAG,IAAI,CAACA,KAAK,CAACkB,MAAM,CAACV,IAAI,IAAIA,IAAI,CAACI,EAAE,KAAKA,EAAE,CAAC;IACxD,CAAC,CAAC,OAAOV,KAAK,EAAE;MACd,IAAI,CAACA,KAAK,GAAGA,KAAK;IACpB;EACF;AACF,CAAC,CAAC","ignoreList":[]}]}