;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA","file":"ManagerPointManagement.vue","sourceRoot":"src/components/manager","sourcesContent":["<template>\n  <div class=\"all\">\n    <ManagerHeader />\n    <div class=\"wrap\">\n      <div class=\"reservation-container\">\n        <ManagerNavbar />\n        <div class=\"wrapper\">\n          <!-- 타이틀 부분 -->\n          <div class=\"title-box\">\n            <h2 class=\"pretendard-bold-normal-black-28px\">포인트 관리</h2>\n          </div>\n\n          <!-- 포인트 관리 내용 -->\n          <div class=\"point-box\">\n            <!-- 탭 버튼 -->\n           <div class=\"tab-buttons\">\n              <div\n                class=\"blue pretendard-regular-normal-white-16px\"\n                @click=\"selectPlan('정액제')\"\n              >\n                정액제\n              </div>\n              <div\n                class=\"yellow pretendard-regular-normal-white-16px\"\n                @click=\"selectPlan('중량제')\"\n              >\n                중량제\n              </div>\n              <div\n                class=\"red pretendard-regular-normal-white-16px\"\n                @click=\"selectPlan('프리패스')\"\n              >\n                프리패스\n              </div>\n            </div>\n\n            <!-- 정액제 사용 정보 -->\n             <!-- 요금제 사용 정보 -->\n            <div class=\"plan-info\">\n              <!-- 정액제 정보 -->\n              <div v-if=\"selectedPlan === '정액제'\">\n                <div class=\"plan-status\">\n                  <div class=\"pretendard-bold-mine-shaft-20px\">정액제 사용중</div>\n                  <button class=\"register-btn pretendard-medium-white-18px\">\n                    결제수단 등록\n                  </button>\n                </div>\n                <div class=\"dates-cost\">\n                  <div class=\"date-info\">\n                    <span class=\"pretendard-regular-normal-mountain-mist-20px\">시작일</span>\n                    <p class=\"start-date pretendard-bold-normal-28px\">2024-07-25</p>\n                  </div>\n                  <div class=\"date-info\">\n                    <span class=\"pretendard-regular-normal-mountain-mist-20px\">만료일</span>\n                    <p class=\"end-date pretendard-bold-normal-28px\">2024-08-24</p>\n                  </div>\n                  <div class=\"cost-info\">\n                    <div>\n                      <span class=\"pretendard-regular-normal-mountain-mist-20px\">이용 비용</span>\n                      <p class=\"cost pretendard-bold-normal-28px\">50,000</p>\n                    </div>\n                    <div>\n                      <button class=\"cost-change-btn\" @click=\"openCostModal\">이용 비용 변경</button>\n                    </div>\n                  </div>\n                </div>\n              </div>\n\n              <!-- 중량제 정보 -->\n              <div v-if=\"selectedPlan === '중량제'\">\n                <div class=\"plan-status\">\n                  <div class=\"pretendard-bold-mine-shaft-20px\">중량제 사용중</div>\n                  <div>\n                    <span class=\"pretendard-bold-mine-shaft-20px\">{{ totalPoint }}P</span>\n                    <button class=\"register-btn pretendard-medium-white-18px\" @click=\"pointChargeModal\">포인트 충전</button>\n                  </div>\n                </div>\n                  <div class=\"dates-cost\">\n                <div class=\"cost-info_2\">\n                  <div class=\"cost-item_2\">\n                    <p class=\"cost pretendard-bold-normal-28px\">10,000P</p>\n                    <span class=\"pretendard-regular-normal-mountain-mist-20px\">9홀당 비용</span>\n                  </div>\n                  <div class=\"cost-item_2\">\n                    <p class=\"cost pretendard-bold-normal-28px\">10,000P</p>\n                    <span class=\"pretendard-regular-normal-mountain-mist-20px\">1인당 비용</span>\n                  </div>\n                </div>\n                  </div>\n              </div>\n\n              <!-- 프리패스 정보 -->\n              <div v-if=\"selectedPlan === '프리패스'\">\n                <div class=\"plan-status\">\n                  <div class=\"pretendard-bold-mine-shaft-20px\">프리패스 사용중</div>\n                   <button class=\"register-btn pretendard-medium-white-18px\">\n                    결제수단 등록\n                  </button>\n                </div>\n                <div class=\"dates-cost\">\n                  <div class=\"dates-cost_3\">\n                  <div class=\"date-info\">\n                    <span class=\"pretendard-regular-normal-mountain-mist-20px\">시작일</span>\n                    <p class=\"start-date pretendard-bold-normal-28px\">2024-07-25</p>\n                  </div>\n                  <div class=\"date-info\">\n                    <span class=\"pretendard-regular-normal-mountain-mist-20px\">만료일</span>\n                    <p class=\"end-date pretendard-bold-normal-28px\">2024-08-24</p>\n                  </div>\n                </div>\n              </div>\n              </div>\n            </div>\n             <div class=\"point-info\" v-if=\"selectedPlan === '프리패스'\">\n              <h4 class=\"pretendard-bold-mine-shaft-20px\">프리패스 사용중 (2024.07 ~ 2024.09)</h4>\n              <ul>\n                <li>\n                 오픈 기념 프로모션으로 3개월 프리패스 서비스를 제공합니다.\n                </li>\n                <li>\n              프로모션은 기간 만료 시 자동 종료되며 정상적인 이용을 위해서 요금제를 반드시 정액제 또는 종량제 선택 후 이용하시기 바랍니다.\n                </li>\n               \n              </ul>\n            </div> \n            <!-- 포인트 요금제 안내 -->\n            <div class=\"point-info\">\n              <h4 class=\"pretendard-bold-mine-shaft-20px\">포인트 요금제 안내</h4>\n              <ul>\n                <li class=\"pretendard-light-granite-gray-16px\">\n                  <strong class=\"pretendard-medium-black-18px\">정액제</strong>- 월별 자동 충전 방식으로 포인트 차감\n                  없이 자유롭게 이용 가능\n                </li>\n                <li class=\"pretendard-light-granite-gray-16px\"> \n                  <strong class=\"pretendard-medium-black-18px\">중량제</strong>- 이용한 이후 포인트와 플레이 유저수를\n                  곱해 초 이외로 포인트를 차감\n                </li>\n                <li class=\"pretendard-light-granite-gray-16px\">\n                  <strong class=\"pretendard-medium-black-18px\">프리패스</strong>- 이벤트형 유료제로 회사 담당자를\n                  통해 진행 가능\n                </li>\n              </ul>\n            </div>\n\n            <!-- 포인트 내역 -->\n            <div class=\"point-history\">\n              <h4>포인트 내역</h4>\n              <div class=\"tabs\">\n                <div\n                  v-for=\"tab in tabs\"\n                  :key=\"tab\"\n                  :class=\"[\n                    'tab-btn pretendard-regular-normal-mine-shaft-16px ',\n                    { active: selectedTab === tab },\n                  ]\"\n                  @click=\"selectTab(tab)\"\n                >\n                  {{ tab }}\n                </div>\n                <input\n                  type=\"date\"\n                  class=\"date pretendard-regular-normal-mine-shaft-18px\"\n                />\n                ~\n                <input\n                  type=\"date\"\n                  class=\"date pretendard-regular-normal-mine-shaft-18px\"\n                />\n              </div>\n              <div class=\"point-list\">\n                <div class=\"table-container\">\n                  <table>\n                    <thead>\n                      <tr class=\"pretendard-bold-mine-shaft-18px \">\n                        <th>번호</th>\n                        <th>내역</th>\n                        <th>분류</th>\n                        <th>금액</th>\n                        <th>잔여 포인트</th>\n                        <th>사용일시</th>\n                      </tr>\n                    </thead>\n                    <tbody>\n                      <tr\n                        v-for=\"(point, index) in paginatedItems\"\n                        :key=\"index\"\n                        class=\"pretendard-light-granite-gray-16px\"\n                      >\n                        <td class=\"pretendard-regular-normal-granite-gray-18px\">{{ point.number }}</td>\n                        <td>{{ point.name }}</td>\n                        <td class=\"pretendard-regular-normal-granite-gray-18px\">{{ point.classification }}</td>\n                        <td :class=\"{'positive-money': isPositiveMoney(point.money), 'negative-money': !isPositiveMoney(point.money)}\">\n                          {{ point.money }}\n                        </td>\n                        <td class=\"pretendard-regular-normal-granite-gray-18px\">{{ point.restpoint }}</td>\n                        <td>{{ point.date }}</td>\n                      </tr>\n                    </tbody>\n                  </table>\n                   <div class=\"pagination-controls\">\n                <img\n                  class=\"pagination-arrow\"\n                  @click=\"prevPage\"\n                  :src=\"arrowLeftImage\"\n                  :alt=\"'Previous'\"\n                  :class=\"{ disabled: currentPage === 1 }\"\n                />\n                <div class=\"page-numbers\">\n                  <div\n                    v-for=\"page in totalPages\"\n                    :key=\"page\"\n                    :class=\"['page-number', { active: page === currentPage }]\"\n                    @click=\"goToPage(page)\"\n                  >\n                    {{ page }}\n                  </div>\n                </div>\n                <img\n                  class=\"pagination-arrow flipped\"\n                  @click=\"nextPage\"\n                  :src=\"arrowRightImage\"\n                  :alt=\"'Next'\"\n                  :class=\"{ disabled: currentPage === totalPages }\"\n                />\n              </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n      <PointChargeModal v-if=\"isModalVisible\" @close=\"closeModal\" />\n      <ManagerUsingCostModal v-if=\"isModalCVisible\" @cClose=\"closeCModal\" />\n    </div>\n  </div>\n</template>\n\n<script>\nimport ManagerHeader from \"./ManagerHeader.vue\";\nimport ManagerNavbar from \"./ManagerNavbar.vue\";\nimport PointChargeModal from \"./PointChargeModal.vue\";\nimport ManagerUsingCostModal from \"./ManagerUsingCostModal.vue\";\n\nexport default {\n  name: \"ManagerPointManagement\",\n  components: {\n    ManagerHeader,\n    ManagerNavbar,\n    PointChargeModal,\n    ManagerUsingCostModal,\n\n  },\n  data() {\n    return {\n      selectedTab: \"전체\",\n       selectedPlan: \"정액제\",\n      tabs: [\"전체\", \"주간\", \"월별\", \"월간\"],\n        totalPoint: 70000, // 포인트 정보 (중량제에서 사용)\n        isModalVisible:false,\n        isModalCVisible:false,\n      point:[\n         { number: 6, name: \"포인트 충전\", classification: \"충전\", money: \"+30,000\", restpoint: \"70,000\", date: \"2024-06-25 23:59:59\" },\n        { number: 5, name: \"9홀당 지불 요금\", classification: \"사용\", money: \"-30,000\", restpoint: \"120,000\", date: \"2024-06-25 23:59:59\"},\n        { number: 4, name: \"포인트 충전\", classification: \"충전\", money: \"+30,000\", restpoint: \"170,000\", date: \"2024-06-25 23:59:59\" },\n        { number: 3, name: \"9홀당 지불 요금\", classification: \"사용\", money:\"-30,000\", restpoint: \"50,000\", date: \"2024-06-25 23:59:59\" },\n        { number: 2, name: \"9홀당 지불 요금\", classification: \"사용\", money: \"+30,000\", restpoint: \"70,000\", date: \"2024-06-25 23:59:59\" },\n        { number: 1, name: \"9홀당 지불 요금\", classification: \"사용\", money: \"+30,000\", restpoint: \"70,000\", date: \"2024-06-25 23:59:59\" }\n      ],\n          arrowLeftImage:\n        \"https://cdn.animaapp.com/projects/66da512d0c22d478015e0e07/releases/66da51d8cf50153cef9952df/img/iconamoon-arrow-up-2-thin.svg\", // 왼쪽 화살표 이미지 URL\n      arrowRightImage:\n        \"https://cdn.animaapp.com/projects/66da512d0c22d478015e0e07/releases/66da51d8cf50153cef9952df/img/iconamoon-arrow-up-2-thin.svg\", // 오른쪽 화살표 이미지 URL\n              currentPage: 1,\n      itemsPerPage: 5, // Display 6 courses per page\n    };\n  },\n    computed: {\n    totalPages() {\n      return Math.ceil(this.point.length / this.itemsPerPage);\n    },\n    paginatedItems() {\n      const start = (this.currentPage - 1) * this.itemsPerPage;\n      return this.point.slice(start, start + this.itemsPerPage);\n    },\n    },\n  methods: {\n    selectTab(tab) {\n      this.selectedTab = tab;\n    },\n      isPositiveMoney(money) {\n      return money.startsWith(\"+\");\n    },\n     selectPlan(plan) {\n      this.selectedPlan = plan; // 선택한 플랜을 업데이트\n    },\n    pointChargeModal(){\n        this.isModalVisible=true;\n    },\n      closeModal(){\n        this.isModalVisible=false;\n    },\n    openCostModal(){\nthis.isModalCVisible=true;\n    },\n    closeCModal(){\n          this.isModalCVisible=false;\n    },\n        nextPage() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++;\n      }\n    },\n    prevPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n      }\n    },\n    goToPage(page) {\n      this.currentPage = page;\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* 전체 컨테이너의 배경색 설정 */\n.reservation-container {\n  background: #f5f6f6;\n  display: flex;\n  height: 100%;\n  flex-direction: row;\n}\n\n.wrapper {\n  flex-grow: 1;\n  padding: 30px;\n  gap: 20px;\n}\n\n/* 타이틀 박스 */\n.title-box {\n  background-color: #fff;\n  padding: 30px;\n  margin-bottom: 20px;\n}\n\n/* 포인트 박스 */\n.point-box {\n  background-color: #fff;\n  padding: 20px;\n  border-radius: 8px;\n  box-shadow: 0px 4px 8px rgba(0, 0, 0, 0.1);\n}\n\n/* 탭 버튼 */\n.tab-buttons {\n  display: flex;\n  gap: 10px;\n  margin-bottom: 20px;\n}\n\n.tab-buttons div {\n  padding: 10px 20px;\n  border: none;\n  color: #fff;\n  border-radius: 5px;\n  cursor: pointer;\n}\n\n.blue {\n  background-color: #184199;\n}\n\n.yellow {\n  background-color: #f4bc1c;\n}\n\n.red {\n  background-color: #d30f1a;\n}\n\n/* 정액제 사용 정보 */\n.plan-info {\n  margin-bottom: 20px;\n}\n\n.plan-status {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  width: 100%;\n  padding: 16.5px 30px;\n  border: 1px solid #ddd;\n  background-color: #f9f9f9;\n  margin: 0;\n}\n\n.register-btn {\n  background-color: #184199;\n  color: white;\n  padding: 12px 15px;\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n}\n\n.dates-cost {\n  display: flex;\n  justify-content: space-between;\n  border: 1px solid #ddd;\n  padding: 27px 30px;\n  gap: 20px;\n  align-items: center;\n}\n.dates-cost_3{\n      display: flex;\n  justify-content: space-between;\n  width:60%;\n  gap: 20px;\n  align-items: center;\n}\n.date-info .start-date,\n.date-info .end-date {\n  color: #d30f1a;\n}\n\n.cost-info {\n  display: flex;\n  gap: 50px;\n}\n.cost-info_2 {\n  display: flex;\n  width:60%;\n  justify-content:space-between;\n}\n.cost-info_2 .cost-item_2 {\n    display: flex;\n    align-items: center;\n    gap:16px;}\n\n.cost-change-btn {\n  background-color: #333;\n  color: white;\n  padding: 8px 16px;\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n  margin-top: 5px;\n}\n\n/* 포인트 요금제 안내 */\n.point-info {\n  padding: 20px;\n}\n\n.point-info h4 {\n  font-weight: bold;\n  margin-bottom: 10px;\n}\n\n.point-info ul {\n  padding: 0;\n}\n\n.point-info li {\n  margin-bottom: 10px;\n}\n\n/* 포인트 내역 */\n.tabs {\n  background-color: #f5f5f5;\n  margin-top: 20px;\n  display: flex;\n  gap: 10px;\n  margin-bottom: 30px;\n  padding: 30px;\n}\n.tab-btn {\n  padding: 23.5px 30px;\n  gap: 10px;\n  background-color: #fff;\n}\n.tab-btn:active {\n  background-color: #184199;\n  color: #fff;\n}\n.point-history {\n  margin-top: 30px;\n}\n\n.point-history h4 {\n  font-size: 16px;\n  font-weight: bold;\n}\n\n.table-container {\n  background-color: #fff;\n  width: 100%;\n  overflow-x: auto;\n}\n\ntable {\n  width: 100%;\n  border-collapse: collapse;\n}\n\nth,\ntd {\n  padding: 26px 0px;\n  border-bottom: 1px solid #ddd;\n  text-align: center;\n}\n\nth {\n  background-color: #f5f5f5;\n}\n.positive-money {\n  color: #184199;\n}\n\n/* 네거티브 금액: 빨간색 */\n.negative-money {\n  color: #d30f1a;\n}\n\n.pagination-controls {\n  display: flex;\n  justify-content: center;\n  gap: 20px;\n  margin-top: 20px;\n}\n\n.page-number.active {\n  background-color: red;\n  color: white;\n  border-radius: 50%;\n  width: 40px;\n  height: 40px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.pagination-arrow {\n  cursor: pointer;\n}\n\n.pagination-arrow.disabled {\n  opacity: 0.5;\n  cursor: not-allowed;\n}\n\n.pagination-arrow.flipped {\n  transform: rotate(180deg);}\n</style>\n"]}]}