lteredCardsEnded() {\n      return this.cards.filter((card) => card.type === \"ended\");\n    },\n    totalPages() {\n      return Math.ceil(this.cards.length / this.itemsPerPage);\n    },\n    paginatedCards() {\n      const start = (this.currentPage - 1) * this.itemsPerPage;\n      return this.cards.slice(start, start + this.itemsPerPage);\n    },\n  },\n  methods: {\n    selectTab(tab) {\n      this.selectedTab = tab;\n    },\n    nextPage() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++;\n      }\n    },\n    prevPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n      }\n    },\n    goToPage(page) {\n      this.currentPage = page;\n    },\n    toggleSelectCard(index) {\n      // 다른 카드는 해제하고 클릭한 카드만 선택\n      this.cards.forEach((card, i) => {\n        if (i === index) {\n          card.isSelected = !card.isSelected;\n        } else {\n          card.isSelected = false;\n        }\n      });\n    },\n    editCard(cardId) {\n      console.log(\"카드 수정:\", cardId);\n      this.$router.push({ path: \"/managergame-management\" })\n      // 여기에 수정하기 로직을 추가합니다.\n    },\n    getShopInfo(shop) {\n      if (shop.length <= 3) {\n        return shop;\n      } else {\n        return [...shop.slice(0, 3), `외 ${shop.length - 3}개`];\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* 전체 컨테이너의 배경색 설정 */\n.reservation-container {\n  background: #f5f6f6;\n  display: flex;\n  height: 100%;\n  /* 기본값이 세로방향인 flex-direction을 가로방향으로 설정 */\n  flex-direction: row;\n}\n\n.wrapper {\n  flex-grow: 1;\n  /* 나머지 공간을 차지하게 함 */\n  padding: 30px;\n  gap: 20px;\n}\n\n/* 타이틀 박스 */\n.title-box {\n  background-color: #fff;\n  padding: 30px;\n  margin-bottom: 20px;\n}\n\n/* 방 리스트 박스 */\n.room-list-box {\n  width: 100%;\n  flex-wrap: wrap;\n  gap: 30px;\n  background-color: #fff;\n  padding: 15px 30px;\n}\n\n.table_box {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 40px;\n}\n\n.card_box {\n  padding: 30px;\n  background-color: #fff;\n}\n\n