    }));\n\n        console.log(\"Processed courses:\", this.courses);\n      } catch (error) {\n        console.error(\"Failed to fetch events data:\", error);\n      }\n    },\n    selectTab(tab) {\n      this.selectedTab = tab;\n      this.currentPage = 1; // Reset to the first page when tab is changed\n    },\n    nextPage() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++;\n      }\n    },\n    prevPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n      }\n    },\n    goToPage(page) {\n      this.currentPage = page;\n    },\n    getLabelStyle(type) {\n      if (type === '진행중') {\n        return {\n          backgroundColor: '#D30F1A',\n          color: 'white',\n          padding: '15px 30px',\n          position: 'absolute',\n          top: '30px',\n          left: '30px',\n          borderRadius: '30px',\n        };\n      } else if (type === '종료') {\n        return {\n          backgroundColor: '#444444',\n          color: 'white',\n          padding: '15px 30px',\n          position: 'absolute',\n          top: '30px',\n          left: '30px',\n          borderRadius: '30px',\n        };\n      }\n    },\n    toGoEventDetail(courseId) {\n      console.log(\"Navigating to event detail with ID:\", courseId); // 콘솔에 id 출력\n      this.$router.push({\n        path: \"/event-detail\",\n        query: { id: courseId }\n      });\n    }\n  },\n};\n</script>\n\n<style scoped>\n.events-2 {\n  width: 100%;\n}\n\n.title_center {\n  align-items: center;\n  padding: 0px 13.5%;\n  justify-content: space-between;\n  display: flex;\n  padding-top: 104px;\n}\n.event-register{\n  width:68px;\n  border-radius: 3px;\n  text-align: center;\n  padding:10px;\n  color:#fff;\n  background-color:#D30F1A;\n}\n.events_wrap {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  margin-bottom: 120px;\n  width: 100%;\n  padding: 0px 13.5%;\n}\n\n.frame-7820 {\n  padding: 40px;\n  width: 100%;\n  border: 1px solid #dd