lass=\"reservation-container\">\n                <AdminNavbar />\n                <div class=\"wrapper\">\n                    <!-- 타이틀 부분 -->\n                    <div class=\"title-box\">\n                        <h2 class=\"pretendard-bold-normal-black-28px\">대회 관리</h2>\n                    </div>\n                    <!-- Select box 부분 -->\n                    <div class=\"select-box\">\n                        <select class=\"text-2536 pretendard-regular-normal-mine-shaft-18px\">\n                            <option value=\"\" disabled selected>전체등급</option>\n                            <option value=\"tournament1\">등급 1</option>\n                            <option value=\"tournament2\">등급 2</option>\n                            <option value=\"tournament3\">등급 3</option>\n                        </select>\n\n                        <select class=\"text-2536 pretendard-regular-normal-mine-shaft-18px\">\n                            <option value=\"\" disabled selected>전체성별</option>\n                            <option value=\"tournament1\">남자</option>\n                            <option value=\"tournament2\">여자</option>\n                        </select>\n                        <select class=\"text-2536 pretendard-regular-normal-mine-shaft-18px\">\n                            <option value=\"\" disabled selected>전체나이</option>\n                            <option value=\"tournament1\">나이1</option>\n                            <option value=\"tournament2\">나이2</option>\n                        </select>\n                    </div>\n\n                    <div class=\"list-box\">\n                        <div class=\"course_title pretendard-bold-log-cabin-24px\">코스별 기록</div>\n                        <div class=\"course-list\">\n                            <div class=\"table-container\">\n                                <table>\n                                    <thead>\n                                        <tr class=\"pretendard-bold-mine-shaft-18px \">\n                                            <th>번호</th>\n                                            <th>코스명</th>\n                                            <th>난이도</th>\n                                            <th>최고</th>\n                                            <th>평균</th>\n                                            <th>스코어카드</th>\n                                        </tr>\n                                    </thead>\n                                    <tbody>\n                                        <tr v-for=\"(game, index) in paginatedItems\" :key=\"index\"\n                                            class=\"pretendard-light-granite-gray-16px\">\n                                            <td class=\"pretendard-regular-normal-granite-gray-18px\">{{ game.number }}\n                                            </td>\n                                            <td class=\"pretendard-regular-normal-granite-gray-18px\">{{ game.name }}</td>\n                                            <td class=\"pretendard-regular-normal-granite-gray-18px\">\n                                                <div class=\"game-difficulty\">\n                                                    <div class=\"text-1609\">코스난이도</div>\n                                                    <!-- 별점 표시 -->\n                                                    <div class=\"rating-stars\">\n                                                        <span v-for=\"star in 5\" :key=\"star\" :class=\"star <= game.difficulty ? 'red-star' : 'gray-star'\n                                                            \">\n                                                            ★\n                                                        </span>\n                                                    </div>\n                                                </div>\n                                            </td>\n                                            <td>\n                                                {{ game.highest }}\n                                            </td>\n                                            <td class=\"pretendard-regular-normal-granite-gray-18px\">{{ game.average }}\n                                            </td>\n                                            <td><button class=\"game-list-btn\" @click=\"AdminScoreCardModal\">상세보기</button>\n                                            </td>\n                                        </tr>\n                                    </tbody>\n                                </table>\n                                <div class=\"pagination-controls\">\n                                    <img class=\"pagination-arrow\" @click=\"prevPage\" :src=\"arrowLeftImage\"\n                                        :alt=\"'Previous'\" :class=\"{ disabled: currentPage === 1 }\" />\n                                    <div class=\"page-numbers\">\n                                        <div v-for=\"page in totalPages\" :key=\"page\"\n                                            :class=\"['page-number', { active: page === currentPage }]\"\n                                            @click=\"goToPage(page)\">\n                                            {{ page }}\n                                        </div>\n                                    </div>\n                                    <img class=\"pagination-arrow flipped\" @click=\"nextPage\" :src=\"arrowRightImage\"\n                                        :alt=\"'Next'\" :class=\"{ disabled: currentPage === totalPages }\" />\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <AdminScorecardModal v-if=\"isModalVisible\" @close=\"closeModal\" />\n        </div>\n    </div>\n</template>\n<script>\nimport AdminHeader from \"./AdminHeader.vue\";\nimport AdminNavbar from \"./AdminNavbar.vue\";\nimport AdminScorecardModal from \"./AdminScorecardModal.vue\";\n\nexport default {\n    name: \"AdminGameManagement\",\n    components: {\n        AdminHeader,\n        AdminNavbar,\n        AdminScorecardModal\n    },\n    data() {\n        return {\n            isModalVisible: false,\n            game: [\n                { number: 6, name: \"스카이뷰CC\", difficulty: 3, highest: \"100타\", average: \"80타\" },\n                { number: 5, name: \"하이원CC\", difficulty: 2, highest: \"100타\", average: \"80타\" },\n                { number: 4, name: \"앰스클럽 의성\", difficulty: 4, highest: \"100타\", average: \"80타\" },\n                { number: 3, name: \"동원썬밸리CC\", difficulty: 3, highest: \"100타\", average: \"80타\" },\n                { number: 2, name: \"사우스프랑스CC\", difficulty: 3, highest: \"100타\", average: \"80타\" },\n                { number: 1, name: \"사우스프랑스CC\", difficulty: 3, highest: \"100타\", average: \"80타\" },\n            ],\n            arrowLeftImage:\n                \"https://cdn.animaapp.com/projects/66da512d0c22d478015e0e07/releases/66da51d8cf50153cef9952df/img/iconamoon-arrow-up-2-thin.svg\", // 왼쪽 화살표 이미지 URL\n            arrowRightImage:\n                \"https://cdn.animaapp.com/projects/66da512d0c22d478015e0e07/releases/66da51d8cf50153cef9952df/img/iconamoon-arrow-up-2-thin.svg\", // 오른쪽 화살표 이미지 URL\n            currentPage: 1,\n            itemsPerPage: 5, // Display 6 courses per page\n        }\n    },\n    computed: {\n        totalPages() {\n            return Math.ceil(this.game.length / this.itemsPerPage);\n        },\n        paginatedItems() {\n            const start = (this.currentPage - 1) * this.itemsPerPage;\n            return this.game.slice(start, start + this.itemsPerPage);\n        },\n    },\n    methods: {\n        nextPage() {\n            if (this.currentPage < this.totalPages) {\n                this.currentPage++;\n            }\n        },\n        prevPage() {\n            if (this.currentPage > 1) {\n                this.currentPage--;\n            }\n        },\n        goToPage(page) {\n            this.currentPage = page;\n        },\n        AdminScoreCardModal() {\n            this.isModalVisible = true;\n        },\n        closeModal() {\n            this.isModalVisible = false;\n        },\n    }\n};\n</script>\n\n<style scoped>\n/* 전체 컨테이너의 배경색 설정 */\n.reservation-container {\n    background: #f5f6f6;\n    display: flex;\n    height: 100%;\n    /* 기본값이 세로방향인 flex-direction을 가로방향으로 설정 */\n    flex-direction: row;\n}\n\n.wrapper {\n    flex-grow: 1;\n    /* 나머지 공간을 차지하게 함 */\n    padding: 30px;\n    gap: 20px;\n}\n\n/* 타이틀 박스 */\n.title-box {\n    background-color: #fff;\n    padding: 30px;\n    margin-bottom: 20px;\n}\n\n/* 방 리스트 박스 */\n.room-list-box {\n    width: 100%;\n    flex-wrap: wrap;\n    gap: 30px;\n    background-color: #fff;\n    padding: 15px 30px;\n}\n\n.table_box {\n    display: flex;\n    flex-wrap: wrap;\n    gap: 40px;\n}\n\nselect {\n    align-items: center;\n    background-color: var(--blackwhitewhite);\n    display: flex;\n    height: 60px;\n    justify-content: space-between;\n    padding: 0px 20px;\n    position: relative;\n    width: 250px;\n    border: 1px solid #ddd;\n    margin-left: 12px;\n}\n\n.select-box {\n    align-items: center;\n    display: flex;\n    gap: 10px;\n    padding: 30px;\n    position: relative;\n    width: 100%;\n    height: 120px;\n    background-color: #fff;\n    margin-bottom: 20px;\n}\n.course_title{\n    margin-bottom:20px;\n}\n.list-box {\n    background-color: #fff;\n    padding: 30px;\n}\n\n.list-box h4 {\n    margin-bottom: 10px;\n}\n\n.table-container {\n    background-color: #fff;\n    width: 100%;\n    overflow-x: auto;\n}\n\ntable {\n    width: 100%;\n    border-collapse: collapse;\n}\n\nth,\ntd {\n    padding: 26px 0px;\n    border-bottom: 1px solid #ddd;\n    text-align: center;\n}\n\nth {\n    background-color: #f5f5f5;\n}\n\n.rating-stars {\n    display: flex;\n    gap: 3px;\n}\n\n.red-star {\n    color: #d30f1a;\n}\n\n.gray-star {\n    color: #ddd;\n}\n\n.game-difficulty {\n    display: flex;\n    justify-content: center;\n    gap: 10px;\n}\n\n.game-list-btn {\n    background-color: #fff;\n    color: #184199;\n    padding: 4px 12px;\n    border: 1px solid #184199;\n    border-radius: 5px;\n    cursor: pointer;\n    margin-top: 5px;\n    height: 32px\n}\n\n.pagination-controls {\n    display: flex;\n    justify-content: center;\n    gap: 20px;\n    margin-top: 20px;\n}\n\n.page-number.active {\n    background-color: red;\n    color: white;\n    border-radius: 50%;\n    width: 40px;\n    height: 40px;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n}\n\n.pagination-arrow {\n    cursor: pointer;\n}\n\n.pagination-arrow.disabled {\n    opacity: 0.5;\n    cursor: not-allowed;\n}\n\n.pagination-arrow.flipped {\n    transform: rotate(180deg);\n}\n</style>\n"]}]}