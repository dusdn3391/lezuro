iv class=\"dashboard_wrap\">\n            <div class=\"admin-dashboard\">\n              <div class=\"board-list-box\">\n                <!-- 코스 랭킹 -->\n                <div class=\"list-box\">\n                  <div class=\"pretendard-bold-mine-shaft-18px\">코스 랭킹</div>\n                  <div class=\"more-link pretendard-light--mountain-mist-16px\">\n                    <a href=\"#\">+ 더보기</a>\n                  </div>\n                </div>\n                <table class=\"course-ranking-table\">\n                  <thead>\n                    <tr>\n                      <th>선호도</th>\n                      <th>코스명</th>\n                      <th>난이도</th>\n                      <th>최고</th>\n                      <th>평균</th>\n                    </tr>\n                  </thead>\n                  <tbody>\n                    <tr v-for=\"course in courses\" :key=\"course.id\">\n                      <td>{{ course.rank }}</td>\n                      <td>{{ course.name }}</td>\n                      <td>\n                        코스난이도\n                        <span\n                          v-for=\"n in course.difficulty\"\n                          :key=\"n\"\n                          class=\"star\"\n                          >★</span\n                        >\n                        <span\n                          v-for=\"n in 5 - course.difficulty\"\n                          :key=\"`empty-${n}`\"\n                          class=\"empty-star\"\n                          >☆</span\n                        >\n                      </td>\n                      <td>{{ course.bestScore }}</td>\n                      <td>{{ course.avgScore }}</td>\n                    </tr>\n                  </tbody>\n                </table>\n              </div>\n            </div>\n            <!-- 포인트 내역 -->\n            <div class=\"admin-dashboard\">\n              <div class=\"board-list-box\">\n                <!-- 코스 랭킹 -->\n                <div class=\"list-box\">\n     