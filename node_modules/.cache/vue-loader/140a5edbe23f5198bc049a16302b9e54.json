<option value=\"6\">최근 50경기</option>\n                  </select>\n                </div>\n                <table>\n                  <thead>\n                    <tr>\n                      <th v-for=\"(header, index) in TableHeaders\" :key=\"index\">\n                        {{ header }}\n                      </th>\n                    </tr>\n                  </thead>\n                  <tbody>\n                    <tr v-for=\"(row, rowIndex) in AvgTableData\" :key=\"rowIndex\">\n                      <td v-for=\"(cell, cellIndex) in row\" :key=\"cellIndex\">\n                        {{ cell }}\n                      </td>\n                    </tr>\n                  </tbody>\n                </table>\n              </div>\n            </div>\n          </div>\n        </div>\n        <div class=\"game\">\n          <div class=\"game_tab\">\n            <div v-for=\"(tab, index) in tabs\" :key=\"index\" @click=\"selectTab(tab)\"\n              :class=\"['tab', { active: selectedTab === tab }]\">\n              {{ tab }}\n              <!-- 데이터 개수 표시 -->\n              <span v-if=\"tab === '전체 경기'\">{{\n                totalEntireGameData\n              }}</span>\n              <span v-if=\"tab === '일반 경기'\">{{\n                totalGameCompetitionData\n              }}</span>\n              <span v-if=\"tab === '대회 경기'\">{{\n                totalCompetitionGameData\n              }}</span>\n            </div>\n          </div>\n          <div class=\"game_table\">\n            <!-- 전체 경기 탭 -->\n            <div v-if=\"selectedTab === '전체 경기'\" class=\"table_box\">\n              <table class=\"table\">\n                <thead>\n                  <tr>\n                    <th v-for=\"(header, index) in CTableHeaders\" :key=\"index\">\n                      {{ header }}\n                    </th>\n                  </tr>\n                </thead>\n                <tbody>\n                  <tr v-for=\"(row, rowIndex) in paginatedEntireGameData\" :key=\"rowIndex\">\n                    <td v-for=\"(cell, cellIndex) in row\" :key=\"cellIndex\">\n                      <span @click=\"openModal(row)\">\n                        {{ cell }}\n                      </span>\n                    </td>\n                  </tr>\n                </tbody>\n              </table>\n            </div>\n            <div class=\"game-list\" v-if=\"selectedTab === '전체 경기'\">\n              <div v-for=\"(game, index) in paginatedEntireGameData\" :key=\"index\" class=\"game-item\">\n                <div class=\"game-info\">\n                  <div class=\"game-info-title\">\n                    <h3 class=\"pretendard-semi-bold-log-cabin-18px\">\n                      {{ game.location }}\n                    </h3>\n                    <span class=\"game-date\">{{ game.date }}</span>\n                  </div>\n                  <div class=\"game-details\">\n                    <span class=\"pretendard-regular-normal-granite-gray-14px\">코스\n                      <div class=\"game-info-line\"></div>\n                      {{ game.course }}\n                    </span>\n                    <span class=\"pretendard-regular-normal-granite-gray-14px\">홀\n                      <div class=\"game-info-line\"></div>\n                      {{ game.holes }}\n                    </span>\n                    <span class=\"pretendard-regular-normal-granite-gray-14px\">타수\n                      <div class=\"game-info-line\"></div>\n                      {{ game.totalScore }}\n                    </span>\n                  </div>\n                  <button class=\"score-card-btn pretendard-regular-normal-tuatara-14px\" @click=\"openModal(game)\">\n                    스코어 카드 보기\n                  </button>\n                </div>\n              </div>\n            </div>\n            <!-- 일반 경기 탭 -->\n            <div v-if=\"selectedTab === '일반 경기'\" class=\"table_box\">\n              <table class=\"table\">\n                <thead>\n                  <tr>\n                    <th v-for=\"(header, index) in CTableHeaders\" :key=\"index\">\n                      {{ header }}\n                    </th>\n                  </tr>\n                </thead>\n                <tbody>\n                  <tr v-for=\"(row, rowIndex) in paginatedGameCompetitionData\" :key=\"rowIndex\">\n                    <td v-for=\"(cell, cellIndex) in row\" :key=\"cellIndex\">\n                      <span @click=\"openModal(row)\">\n                        {{ cell }}\n                      </span>\n                    </td>\n                  </tr>\n                </tbody>\n              </table>\n            </div>\n            <div class=\"game-list\" v-if=\"selectedTab === '일반 경기'\">\n              <div v-for=\"(game, index) in paginatedGameCompetitionData\" :key=\"index\" class=\"game-item\">\n                <div class=\"game-info\">\n                  <div class=\"game-info-title\">\n                    <h3 class=\"pretendard-semi-bold-log-cabin-18px\">{{ game.location }}</h3>\n                    <span class=\"game-date\">{{ game.date }}</span>\n                  </div>\n                  <div class=\"game-details\">\n                    <span class=\"pretendard-regular-normal-granite-gray-14px\">코스 <div class=\"game-info-line\"></div>{{\n                      game.course }}</span>\n                    <span class=\"pretendard-regular-normal-granite-gray-14px\">홀 <div class=\"game-info-line\"></div>{{\n                      game.holes }}</span>\n                    <span class=\"pretendard-regular-normal-granite-gray-14px\">타수 <div class=\"game-info-line\"></div>{{\n                      game.totalScore }}</span>\n                  </div>\n                  <button class=\"score-card-btn pretendard-regular-normal-tuatara-14px\" @click=\"openModal(game)\">스코어 카드\n                    보기</button>\n                </div>\n              </div>\n            </div>\n            <!-- 대회 경기 탭 -->\n            <div v-if=\"selectedTab === '대회 경기'\" class=\"table_box\">\n              <table class=\"table\">\n                <thead>\n                  <tr>\n                    <th v-for=\"(header, index) in CTableHeaders\" :key=\"index\">\n                      {{ header }}\n                    </th>\n                  </tr>\n                </thead>\n                <tbody>\n                  <tr v-for=\"(row, rowIndex) in paginatedCompetitionGameData\" :key=\"rowIndex\">\n                    <td v-for=\"(cell, cellIndex) in row\" :key=\"cellIndex\">\n                      <span @click=\"openModal(row)\">\n                        {{ cell }}\n                      </span>\n                    </td>\n                  </tr>\n                </tbody>\n              </table>\n            </div>\n            <div class=\"game-list\" v-if=\"selectedTab === '대회 경기'\">\n              <div v-for=\"(game, index) in paginatedCompetitionGameData\" :key=\"index\" class=\"game-item\">\n                <div class=\"game-info\">\n                  <div class=\"game-info-title\">\n                    <h3 class=\"pretendard-semi-bold-log-cabin-18px\">\n                      {{ game.location }}\n                    </h3>\n                    <span class=\"game-date\">{{ game.date }}</span>\n                  </div>\n                  <div class=\"game-details\">\n                    <span class=\"pretendard-regular-normal-granite-gray-14px\">코스\n                      <div class=\"game-info-line\"></div>\n                      {{ game.course }}\n                    </span>\n                    <span class=\"pretendard-regular-normal-granite-gray-14px\">홀\n                      <div class=\"game-info-line\"></div>\n                      {{ game.holes }}\n                    </span>\n                    <span class=\"pretendard-regular-normal-granite-gray-14px\">타수\n                      <div class=\"game-info-line\"></div>\n                      {{ game.totalScore }}\n                    </span>\n                  </div>\n                  <button class=\"score-card-btn pretendard-regular-normal-tuatara-14px\" @click=\"openModal(game)\">\n                    스코어 카드 보기\n                  </button>\n                </div>\n              </div>\n            </div>\n          </div>\n          <!-- 페이징 버튼들 -->\n          <div class=\"paging\" v-show=\"selectedTab === '전체 경기'\">\n            <button @click=\"prevPage('entire')\" :disabled=\"currentPageEntire === 1\" class=\"paging-button\">\n              <img\n                src=\"https://cdn.animaapp.com/projects/66da512d0c22d478015e0e07/releases/66da51d8cf50153cef9952df/img/iconamoon-arrow-up-2-thin.svg\"\n                alt=\"이전 페이지\" />\n            </button>\n            <div class=\"paging-numbers\">\n              <div v-for=\"page in totalPagesEntire\" :key=\"page\" @click=\"goToPage(page, 'entire')\" :class=\"[\n                'paging-number',\n                { active: currentPageEntire === page },\n              ]\">\n                {{ page }}\n              </div>\n            </div>\n            <button @click=\"nextPage('entire')\" :disabled=\"currentPageEntire === totalPagesEntire\"\n              class=\"paging-button\">\n              <img\n                src=\"https://cdn.animaapp.com/projects/66da512d0c22d478015e0e07/releases/66da51d8cf50153cef9952df/img/iconamoon-arrow-up-2-thin.svg\"\n                alt=\"다음 페이지\" class=\"flipped\" />\n            </button>\n          </div>\n          <div class=\"paging\" v-show=\"selectedTab === '일반 경기'\">\n            <button @click=\"prevPage('game')\" :disabled=\"currentPageGame === 1\" class=\"paging-button\">\n              <img\n                src=\"https://cdn.animaapp.com/projects/66da512d0c22d478015e0e07/releases/66da51d8cf50153cef9952df/img/iconamoon-arrow-up-2-thin.svg\"\n                alt=\"이전 페이지\" />\n            </button>\n            <div class=\"paging-numbers\">\n              <div v-for=\"page in totalPagesGame\" :key=\"page\" @click=\"goToPage(page, 'game')\" :class=\"[\n                'paging-number',\n                { active: currentPageGame === page },\n              ]\">\n                {{ page }}\n              </div>\n            </div>\n            <button @click=\"nextPage('game')\" :disabled=\"currentPageGame === totalPagesGame\" class=\"paging-button\">\n              <img\n                src=\"https://cdn.animaapp.com/projects/66da512d0c22d478015e0e07/releases/66da51d8cf50153cef9952df/img/iconamoon-arrow-up-2-thin.svg\"\n                alt=\"다음 페이지\" class=\"flipped\" />\n            </button>\n          </div>\n          <div class=\"paging\" v-show=\"selectedTab === '대회 경기'\">\n            <button @click=\"prevPage('competition')\" :disabled=\"currentPageCompetition === 1\" class=\"paging-button\">\n              <img\n                src=\"https://cdn.animaapp.com/projects/66da512d0c22d478015e0e07/releases/66da51d8cf50153cef9952df/img/iconamoon-arrow-up-2-thin.svg\"\n                alt=\"이전 페이지\" />\n            </button>\n            <div class=\"paging-numbers\">\n              <div v-for=\"page in totalPagesCompetition\" :key=\"page\" @click=\"goToPage(page, 'competition')\" :class=\"[\n                'paging-number',\n                { active: currentPageCompetition === page },\n              ]\">\n                {{ page }}\n              </div>\n            </div>\n            <button @click=\"nextPage('competition')\" :disabled=\"currentPageCompetition === totalPagesCompetition\"\n              class=\"paging-button\">\n              <img\n                src=\"https://cdn.animaapp.com/projects/66da512d0c22d478015e0e07/releases/66da51d8cf50153cef9952df/img/iconamoon-arrow-up-2-thin.svg\"\n                alt=\"다음 페이지\" class=\"flipped\" />\n            </button>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <x-footer />\n    <GameScoreCard v-if=\"isModalVisible\" :content=\"modalData\" @closeModal=\"closeModal\" />\n  </div>\n  <!-- </div> -->\n</template>\n\n<script>\nimport { taskStore } from \"@/store/taskStore\"; // API 호출\nimport Header from \"./Header\";\nimport RjZsWi from \"./RjZsWi\";\nimport XFooter from \"./XFooter\";\nimport GameScoreCard from \"./GameScoreCard\";\n\nexport default {\n  name: \"MyScoreCard\",\n  data() {\n    return {\n      level: 1,\n      bestRecordDt: [],\n      tournamentDt: [],\n      TableHeaders: [\"항목\", \"내 기록\", \"등급평균\"],\n      bestTableData: [\n        [\"최종 스코어\", \"50타\", \"64타\"],\n        [\"최장 퍼팅거리\", \"10.08m\", \"15.23m\"],\n        [\"최장 티샷거리\", \"210.9m\", \"244.0m\"],\n      ],\n      bestTableData: [],\n      AvgTableData: [\n        [\"타수\", \"60타\", \"64타\"],\n        [\"타샷거리\", \"10.08m\", \"15.23m\"],\n        [\"퍼팅수\", \"210.9m\", \"244.0m\"],\n      ],\n      CTableHeaders: [\"일시\", \"장소\", \"코스\", \"홀\", \"타수\", \"스코어 카드\"],\n      // gameCompetitionData: [\n      //   { date:'2024-06-25', location:'서울 레저로 파크',course:'레저로 A코스',hole:'16홀',ta: '0타',detail: '상세보기'},\n      //   { date:'2024-06-25', location:'서울 레저로 파크',course:'레저로 A코스',hole:'16홀',ta: '0타',detail: '상세보기'},\n      //   { date:'2024-06-25', location:'서울 레저로 파크',course:'레저로 A코스',hole:'16홀',ta: '0타',detail: '상세보기'},\n      //   { date:'2024-06-25', location:'서울 레저로 파크',course:'레저로 A코스',hole:'16홀',ta: '0타',detail: '상세보기'},\n      //   { date:'2024-06-25', location:'서울 레저로 파크',course:'레저로 A코스',hole:'16홀',ta: '0타',detail: '상세보기'},\n      // ],\n      gameCompetitionData: [], // 빈 배열로 초기화\n      competitionGameData: [],\n      // competitionGameData: [\n      //   {  date: \"2024-06-25\", location: \"서울 레저로 파크\",course: \"레저로 A코스\",\n      //     hole: \"16홀\",\n      //     ta: \"0타\",\n      //     detail: \"상세보기\",\n      //   },\n      //   {\n      //     date: \"2024-06-25\",\n      //     location: \"서울 레저로 파크\",\n      //     course: \"레저로 A코스\",\n      //     hole: \"16홀\",\n      //     ta: \"0타\",\n      //     detail: \"상세보기\",\n      //   },\n      //   {\n      //     date: \"2024-06-25\",\n      //     location: \"서울 레저로 파크\",\n      //     course: \"레저로 A코스\",\n      //     hole: \"16홀\",\n      //     ta: \"0타\",\n      //     detail: \"상세보기\",\n      //   },\n      //   {\n      //     date: \"2024-06-25\",\n      //     location: \"서울 레저로 파크\",\n      //     course: \"레저로 A코스\",\n      //     hole: \"16홀\",\n      //     ta: \"0타\",\n      //     detail: \"상세보기\",\n      //   },\n      //   {\n      //     date: \"2024-06-25\",\n      //     location: \"서울 레저로 파크\",\n      //     course: \"레저로 A코스\",\n      //     hole: \"16홀\",\n      //     ta: \"0타\",\n      //     detail: \"상세보기\",\n      //   },\n      // ], // 10개의 항목 정의\n      currentPage: 1,\n      selectedTab: \"전체 경기\",\n      itemsPerPage: 5, // 한 페이지당 보여줄 데이터 수\n      currentPageEntire: 1,\n      currentPageGame: 1,\n      currentPageCompetition: 1,\n      itemsPerPage: 5,\n      tabs: [\"전체 경기\", \"일반 경기\", \"대회 경기\"],\n      isModalVisible: false,\n      modalData: null,\n      arrowLeftImage:\n        \"https://cdn.animaapp.com/projects/66da512d0c22d478015e0e07/releases/66da51d8cf50153cef9952df/img/iconamoon-arrow-up-2-thin.svg\", // 왼쪽 화살표 이미지 URL\n      arrowRightImage:\n        \"https://cdn.animaapp.com/projects/66da512d0c22d478015e0e07/releases/66da51d8cf50153cef9952df/img/iconamoon-arrow-up-2-thin.svg\", // 오른쪽 화살표 이미지 URL\n    };\n  },\n  components: {\n    GameScoreCard,\n    Header,\n    RjZsWi,\n    XFooter,\n  },\n  props: [\"rJZsWIProps\"],\n  computed: {\n    levelColor() {\n      const colors = [\"gold\", \"silver\", \"brown\", \"green\", \"yellow\", \"red\"];\n      return colors[this.level - 1] || \"default-color\";\n    },\n    levelTitle() {\n      return `등급${this.level} `;\n    },\n    // async getBestScore(courseId) {\n    //   // `/api/games/tournament-record/{courseId}`\n    //   this.bestRecordDt = await taskStore.getTask(\n    //     `/api/games/best-record/${courseId}`\n    //   );\n    //   console.log(\"this.bestRecordDt : : : : : : \", this.bestRecordDt);\n    //   localStorage.setItem(\"bestRecordDt\", JSON.stringify(this.bestRecordDt));\n    // },\n    // async getGameScore(courseId) {\n    //   this.tournamentDt = await taskStore.getTask(\n    //     `/api/games/tournament-record/${courseId}`\n    //   );\n    //   console.log(\"this.tournamentDt : : : : : : \", this.tournamentDt);\n    //   localStorage.setItem(\"tournamentDt\", JSON.stringify(this.tournamentDt));\n    // },\n    paginatedEntireGameData() {\n      const start = (this.currentPageEntire - 1) * this.itemsPerPage;\n      const end = start + this.itemsPerPage;\n      return [...this.gameCompetitionData, ...this.competitionGameData]\n        .slice(start, end)\n        .map(({ id, date, location, course, holes, totalScore }) => ({\n          date,\n          location,\n          course,\n          holes, // hole을 먼저 배치\n          totalScore, // totalscore을 그 다음 배치\n          detail: \"상세보기\" // '상세보기' 추가\n        }));\n    },\n    totalPagesEntire() {\n      return Math.ceil(\n        (this.gameCompetitionData.length + this.competitionGameData.length) /\n        this.itemsPerPage\n      );\n    },\n    paginatedGameCompetitionData() {\n      const start = (this.currentPageGame - 1) * this.itemsPerPage;\n      const end = start + this.itemsPerPage;\n      return this.gameCompetitionData\n        .slice(start, end)\n        .map(({ id, date, location, course, holes, totalScore }) => ({\n          date,\n          location,\n          course,\n          holes, // hole을 먼저 배치\n          totalScore, // totalscore을 그 다음 배치\n          detail: \"상세보기\" // '상세보기' 추가\n        }));\n    },\n    totalPagesGame() {\n      return Math.ceil(this.gameCompetitionData.length / this.itemsPerPage);\n    },\n    paginatedCompetitionGameData() {\n      const start = (this.currentPageCompetition - 1) * this.itemsPerPage;\n      const end = start + this.itemsPerPage;\n      return this.competitionGameData\n        .slice(start, end)\n        .map(({ id, date, location, course, holes, totalScore }) => ({\n          date,\n          location,\n          course,\n          holes, // hole을 먼저 배치\n          totalScore, // totalscore을 그 다음 배치\n          detail: \"상세보기\" // '상세보기' 추가\n        }));\n    },\n\n    totalPagesCompetition() {\n      return Math.ceil(this.competitionGameData.length / this.itemsPerPage);\n    },\n    totalEntireGameData() {\n      return this.gameCompetitionData.length + this.competitionGameData.length;\n    },\n    totalGameCompetitionData() {\n      return this.gameCompetitionData.length;\n    },\n    totalCompetitionGameData() {\n      return this.competitionGameData.length;\n    },\n  },\n  mounted() {\n    this.getCompetitionGameDt();\n    this.getGameCompetitionDt();\n    this.getbestscore();\n  },\n  methods: {\n    async getGameCompetitionDt() {\n      const data = await taskStore.getSelectTasks(`score-cards/list`);\n      // holes와 totalScore에 텍스트 추가\n      this.gameCompetitionData = data.map(item => ({\n        ...item,\n        holes: `${item.holes}홀`, // '홀' 추가\n        totalScore: `${item.totalScore}타` // '타' 추가\n      }));\n      // console.log(\"scorecard : : : \", this.gameCompetitionData);\n    },\n\n    async getCompetitionGameDt() {\n      const data = await taskStore.getSelectTasks(`score-cards/list`);\n      // holes와 totalScore에 텍스트 추가\n      this.competitionGameData = data.map(item => ({\n        ...item,\n        holes: `${item.holes}홀`, // '홀' 추가\n        totalScore: `${item.totalScore}타` // '타' 추가\n      }));\n      // console.log(\"scorecard : : : \", this.competitionGameData);\n    },\n    // async getbestscore() {\n    //   this.bestTableData = await taskStore.getSelectTasks(\n    //     `score-cards/best-record`\n    //   );\n\n    //   console.log(\"bestscorecard : : : \", this.bestTableData);\n    // },\n    openModal(game, row) {\n      console.log(game); // game 데이터 확인\n      (this.modalData = game), row; // modalData에 game 객체 전달\n      this.isModalVisible = true;\n    },\n    closeModal() {\n      th