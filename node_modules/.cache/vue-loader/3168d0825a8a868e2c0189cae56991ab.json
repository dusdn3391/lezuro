agination-arrow flipped\"\n            @click=\"nextPage\"\n            :src=\"arrowRightImage\"\n            alt=\"Next\"\n            :class=\"{ disabled: currentPage === totalPages }\"\n          />\n        </div>\n      </div>\n    </div>\n    <x-footer />\n  </div>\n</template>\n\n<script>\nimport Header from \"./Header\";\nimport RjZsWi from \"./RjZsWi\";\nimport { taskStore } from \"@/store/taskStore\"; \nimport XFooter from \"./XFooter\";\n\nexport default {\n  name: \"BoardList\",\n  components: {\n    Header,\n    RjZsWi,\n    XFooter,\n  },\n  props: [\"rJZsWIProps\"],\n  data() {\n    return {\n      currentPage: 1,\n      itemsPerPage: 5,\n      arrowLeftImage:\n        \"https://cdn.animaapp.com/projects/66da512d0c22d478015e0e07/releases/66da51d8cf50153cef9952df/img/iconamoon-arrow-up-2-thin.svg\",\n      arrowRightImage:\n        \"https://cdn.animaapp.com/projects/66da512d0c22d478015e0e07/releases/66da51d8cf50153cef9952df/img/iconamoon-arrow-up-2-thin.svg\",\n      thItems: [\n        // {\n        //   number: 1,\n        //   title: \"자유게시판 제목입니다 자유게시판 제목입니다\",\n        //   author: \"레저로 박\",\n        //   date: \"2024-08-23\",\n        //   views: 230,\n        // },\n        // {\n        //   number: 2,\n        //   title: \"자유게시판 제목입니다 자유게시판 제목입니다\",\n        //   author: \"레저로 박\",\n        //   date: \"2024-08-23\",\n        //   views: 230,\n        // },\n        // {\n        //   number: 3,\n        //   title: \"자유게시판 제목입니다 자유게시판 제목입니다\",\n        //   author: \"레저로 박\",\n        //   date: \"2024-08-23\",\n        //   views: 230,\n        // },\n        // {\n        //   number: 4,\n        //   title: \"자유게시판 제목입니다 자유게시판 제목입니다\",\n        //   author: \"레저로 박\",\n        //   date: \"2024-08-23\",\n        //   views: 230,\n        // },\n        // {\n        //   number: 5,\n        //   title: \"자유게시판 제목입니다 자유게시판 제목입니다\",\n        //   author: \"레저로 박\",\n        //   date: \"2024-08-23\",\n        //   views: 230,\n        // },\n      ],\n    };\n  },\n  computed: {\n    totalPages() {\n      return Math.ceil(this.thItems.length / this.itemsPerPage);\n    },\n    paginatedItems() {\n      const start = (this.currentPage - 1) * this.itemsPerPage;\n      return this.thItems.slice(start, start + this.itemsPerPage);\n    },\n  },\n  mounted(){\n    this.getBoardList();\n  },\n  methods: {\n    async getBoardList() {\n    try {\n      // API 호출\n      const response = await taskStore.getSelectTasks(`score-cards/list`);\n      console.log(\"API 응답:\", response);\n\n      // 데이터 가공 후 thItems에 할당\n      this.thItems = response.map((item, index) => ({\n        number: index + 1,\n        title: item.title,\n        author: item.author,\n        date: item.createdAt.split('T')[0], // 날짜를 'YYYY-MM-DD' 형식으로 변환\n        views: item.views,\n      }));\n\n      // 가공된 데이터를 콘솔에 출력\n      console.log(\"게시판 데이터:\", this.thItems);\n    } catch (error) {\n      console.error(\"게시판 데이터를 가져오는 중 오류 발생:\", error);\n    }\n  },\n    goToBoardWrite() {\n      this.$router.push({ path: \"/board-write\" });\n    },\n    goToBoardDetail() {\n      this.$router.push({ path: \"/board-detail\" });\n    },\n    nextPage() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++;\n      }\n    },\n    prevPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n      }\n    },\n    goToPage(page) {\n      this.currentPage = page;\n    },\n  },\n};\n</script>\n\n<style scoped>\n.community-2 {\n  width: 100%;\n}\n\n.title_center {\n  width: 100%;\n}\n\n.title {\n  padding: 0px 13.5%;\n  padding-top: 104px;\n  width: 100%;\n}\n\n.community-2 {\n  align-items: flex-start;\n  background-color: var(--blackwhitewhite);\n  display: flex;\n  flex-direction: column;\n  min-height: 1458px;\n  overflow-x: hidden;\n  position: relative;\n  width: 100%;\n}\n\n.frame-7833 {\n  align-items: flex-end;\n  align-self: stretch;\n  display: flex;\n  flex: 0 0 auto;\n  flex-direction: column;\n  gap: 20