      const selectedType = typeMapping[this.selectedTab];\n\n        // `selectedTab`에 해당하는 데이터만 필터링\n        this.termsContent = response.filter((item) => item.type === selectedType);\n      } catch (error) {\n        console.error(\"데이터를 가져오는 중 오류 발생:\", error);\n      }\n    },\n    selectTab(tabName) {\n      this.selectedTab = tabName; // 탭 선택 업데이트\n      this.$router.push({ query: { tab: tabName } }); // URL 쿼리 업데이트\n      this.loadTermsData(); // 데이터 다시 로드\n      this.updateTitle();\n    },\n    updateTitle() {\n      const windowWidth = window.innerWidth;\n      if (windowWidth <= 720) {\n        this.title = this.selectedTab;\n      } else {\n        this.title = \"약관\";\n      }\n    },\n  },\n};\n</script>\n<style>\n.terms-2 {\n  background-color: var(--blackwhitewhite);\n  display: flex;\n  flex-direction: column;\n  position: relative;\n  width: 100%;\n}\n\n.frame-7881 {\n  align-items: flex-start;\n  display: flex;\n  flex-direction: column;\n  gap: 50px;\n  margin-top: 104px;\n  position: relative;\n  width: 100%;\n}\n\n.frame-7880 {\n  align-items: flex-start;\n  align-self: stretch;\n  display: flex;\n  flex: 0 0 auto;\n  flex-direction: column;\n  position: relative;\n  width: 100%;\n}\n\n.frame-7879 {\n  align-items: flex-end;\n  align-self: stretch;\n  display: flex;\n  flex: 0 0 auto;\n  flex-direction: column;\n  gap: 20px;\n  position: relative;\n  width: 100%;\n}\n\n.frame-7691-3 {\n  align-items: flex-start;\n  align-self: stretch;\n  display: flex;\n  flex: 0 0 auto;\n  flex-direction: column;\n  gap: 40px;\n  position: relative;\n  width: 100%;\n}\n\n.group-764-4 {\n  height: 60px;\n  position: relative;\n  width: 1399.98px;\n}\n\n.frame-7727-4 {\n  align-items: center;\n  display: inline-flex;\n  position: relative;\n}\n\n.frame-7838-2 {\n  align-items: flex-end;\n  align-self: stretch;\n  border: 1px solid #ddd;\n  display: flex;\n  flex: 0 0 auto;\n  flex-direction: column;\n  gap: 20px;\n  padding: 40px;\n  justify-content: center;\n  