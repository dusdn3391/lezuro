   <!-- 날짜 선택 부분 -->\n    <div class=\"date-picker-box\">\n      <input type=\"date\" class=\"date pretendard-regular-normal-mine-shaft-18px \" v-model=\"selectedDate\" />\n    </div>\n\n    <!-- 룸 리스트 부분 -->\n    <div class=\"room-list-box\">\n          <div class=\"tabs\">\n      <button \n        v-for=\"tab in tabs\" \n        :key=\"tab\" \n        :class=\"['tab-btn pretendard-semibold-mountain-mist-16px', { active: selectedTab === tab }] \" \n        @click=\"selectTab(tab)\"\n      >\n        {{ tab }}\n      </button>\n    </div>\n\n    <!-- 테이블 리스트 -->\n    <div class=\"table-container\">\n      <table>\n        <thead>\n          <tr>\n            <th><input type=\"checkbox\"></th>\n            <th>번호</th>\n            <th>예약자</th>\n            <th>연락처</th>\n            <th>인원</th>\n            <th>예약시간</th>\n            <th>상태</th>\n            <th>접수 시간</th>\n          </tr>\n        </thead>\n        <tbody>\n          <tr v-for=\"(reservation, index) in paginatedReservations\" :key=\"index\" class=\"pretendard-semibold-mountain-mist-16px \">\n            <td><input type=\"checkbox\"></td>\n            <td>{{ reservation.id }}</td>\n            <td>{{ reservation.reser_user }}</td>\n            <td>{{ reservation.reser_phone }}</td>\n            <td>{{ reservation.member }}</td>\n            <td>{{ reservation.startTime.split('T')[1].slice(0, 5) }} - {{ reservation.endTime.split('T')[1].slice(0, 5) }}</td>\n            <td><span :class=\"statusClass(reservation.status)\">{{ reservation.status === 'success' ? '예약완료' : reservation.status === 'cancel' ? '예약취소' : '접수중'}}</span></td>\n            <td>{{ reservation.startTime.split('T')[0] }}</td>\n          </tr>\n        </tbody>\n      </table>\n    </div>\n        <div class=\"pagination-controls\">\n              <img\n                class=\"pagination-arrow\"\n                @click=\"prevPage\"\n                :src=\"arrowLeftImage\"\n                :alt=\"'Previous'\"\n                :class=\"{ disabled: currentPage === 1 }\"\n              />\n              <div class=\"page-numbers\">\n                <div\n                  v-for=\"page in totalPages\"\n                  :key=\"page\"\n                  :class=\"['page-number', { active: page === currentPage }]\"\n                  @click=\"goToPage(page)\"\n                >\n                  {{ page }}\n                </div>\n              </div>\n              <img\n                class=\"pagination-arrow flipped\"\n                @click=\"nextPage\"\n                :src=\"arrowRightImage\"\n                :alt=\"'Next'\"\n                :class=\"{ disabled: currentPage === totalPages }\"\n              />\n            </div>\n    </div>\n  </div>\n      </div>\n    </div>\n </div>\n</template>\n\n<script>\nimport ManagerHeader from \"./ManagerHeader.vue\";\nimport ManagerNavbar from \"./ManagerNavbar.vue\";\nimport { managerStore } from '@/store/managerStore';  // API 호출\n\nexport default {\n    name:\"ManagerReservation\",\n      components: {\n    ManagerHeader,\n    ManagerNavbar,\n  },\n  data() {\n    return {\n          selectedDate: \"\",\n      selectedTab: \"전체\",\n      tabs: [\"전체\", \"접수중\", \"예약완료\", \"예약취소\"],\n      reservations: [\n        { id: 6, reser_user: \"홍길동\", reser_phone: \"010-1234-1234\", member: 5, startTime: \"07-25 14:00\", status: \"예약취소\", endTime: \"12:50\" },\n        { id: 5, reser_user: \"홍길동\", reser_phone: \"010-1234-1234\", member: 5, startTime: \"07-25 14:00\", status: \"예약취소\", endTime: \"12:50\" },\n        { id: 4, reser_user: \"홍길동\", reser_phone: \"010-1234-1234\", member: 5, startTime: \"07-25 14:00\", status: \"예약취소\", endTime: \"12:50\" },\n        { id: 3, reser_user: \"홍길동\", reser_phone: \"010-1234-1234\", member: 5, startTime: \"07-25 14:00\", status: \"예약취소\", endTime: \"12:50\" },\n        { id: 2, reser_user: \"홍길동\", reser_phone: \"010-1234-1234\", member: 5, startTime: \"07-25 14:00\", status: \"예약취소\", endTime: \"12:50\" },\n\n      ],\n         arrowLeftImage:\n        \"https://cdn.animaapp.com/projects/66da512d0c22d478015e0e07/releases/66da51d8cf50153cef9952df/img/iconamoon-arrow-up-2-thin.svg\", // 왼쪽 화살표 이미지 URL\n      arrowRightImage:\n        \"https://cdn.animaapp.com/projects/66da512d0c22d478015e0e07/releases/66da51d8cf50153cef9952df/img/iconamoon-arrow-up-2-thin.svg\", // 오른쪽 화살표 이미지 URL\n              currentPage: 1,\n      itemsPerPage: 5, // Display 6 courses per page\n         currentPage: 1, // 현재 페이지\n    }\n  },\n  computed: {\n    filteredReservations() {\n      if (this.selectedTab === \"전체\") {\n        return this.reservations;\n      }\n      return this.reservations.filter(reservation => reservation.status === this.selectedTab);\n    },\n    paginatedReservations() {\n      const start = (this.currentPage - 1) * this.itemsPerPage;\n      const end = start + this.itemsPerPage;\n      return this.filteredReservations.slice(start, end);\n    },\n    totalPages() {\n      return Math.ceil(this.filteredReservations.length / this.itemsPerPage);\n    },\n  },\n  mounted(){\n    this.getReservationDt();\n  },\n   methods: {\n    async getReservationDt() {\n    \n      const roomDt = await managerStore.getTask(`reservations`);\n      this.reservations = roomDt\n      console.log(\"this.roomDt : : : : : : \" , JSON.stringify(roomDt))\n    },\n\n    selectTab(tab) {\n      this.selectedTab = tab;\n    },\n    statusClass(status) {\n      if (status === \"available\") return \"status-pending\";\n      if (status === \"success\") return \"status-complete\";\n      if (status === \"cancel\") return \"status-cancel\";\n      return \"\";\n    },\n       nextPage() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++;\n      }\n    },\n    prevPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n      }\n    },\n    goToPage(page) {\n      this.currentPage = page;\n    },\n  }\n};\n</script>\n\n<style scoped>\n/* 전체 컨테이너의 배경색 설정 */\n.reservation-container {\n  background: #f5f6f6;\n  display: flex;\n  height: 100%;\n  /* 기본값이 세로방향인 flex-direction을 가로방향으로 설정 */\n  flex-direction: row;\n}\n.wrapper {\n  flex-grow: 1; /* 나머지 공간을 차지하게 함 */\n  padding: 30px;\n  gap: 20px;\n}\n/* 타이틀 박스 */\n.title-box {\n  background-color: #fff;\n  padding: 30px;\n  margin-bottom: 20px;\n}\n\n/* 날짜 선택 박스 */\n.date-picker-box {\n  background-color: #fff;\n  padding: 30px;\n}\n.date{padding:23.5px 20px}\n/* 방 리스트 박스 */\n.room-list-box {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 30px;\n  background-color: #fff;\n  padding:30px;\n}\n\n.tabs {\n  background-color: #fff;\n  width:40%;\n  display: flex;\n  gap :10px;\n}\n\n.tab-btn {\n  flex: 1;\n  padding: 15px;\n  text-align: center;\n  border: none;\n  background-color: #f5f5f5;\n  cursor: pointer;\n    width:150px;\n}\n\n.tab-btn.active {\n  background-color: #003399;\n  color: #fff;\n}\n\n/* 테이블 스타일 */\n.table-container {\n  background-color: #fff;\n  width:100%;\n  overflow-x: auto;\n}\n\ntable {\n  width: 100%;\n  border-collapse: collapse;\n}\n\nth, td {\n  padding: 26px 0px;\n  border-bottom: 1px solid #ddd;\n  text-align: center;\n  \n}\n\nth {\n  background-color: #f5f5f5;\n}\n\n.status-pending {\n  color: #D30F1A;\n  border: 1px solid #D30F1A;\n  width: 80px;\n  padding: 11px 0px;\n  border-radius: 5px;\n  display: inline-block; /* 셀 안에서 중앙에 위치하도록 변경 */\n  text-align: center; /* 텍스트도 가운데 정렬 */\n}\n\n.status-complete {\n  color: #184199;\n  border: 1px solid #184199;\n  width: 80px;\n  padding: 11px 0px;\n  border-radius: 5px;\n  display: inline-block;\n  text-align: center;\n}\n\n.status-cancel {\n  color: gray;\n  border: 1px solid gray;\n  width: 80px;\n  padding: 11px 0px;\n  border-radius: 5px;\n  display: inline-block;\n  text-align: center;\n}\n.pagination-controls {\n  display: flex;\n  justify-content: center;\n  gap: 20px;\n  margin-top: 20px;\n  width:100%;\n}\n\n.page-number.active {\n  background-color: red;\n  color: white;\n  border-radius: 50%;\n  width: 40px;\n  height: 40px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.pagination-arrow {\n  cursor: pointer;\n}\n\n.pagination-arrow.disabled {\n  opacity: 0.5;\n  cursor: not-allowed;\n}\n\n.pagination-arrow.flipped {\n  transform: rotate(180deg);}\n</style>\n"]}]}