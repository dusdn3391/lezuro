nfo = JSON.parse(localStorage.getItem('managerInfo'));\n    console.log(\"managerInfo : : : \" , JSON.stringify(managerInfo))\n    this.managerName = managerInfo.name;\n    this.storeName = managerInfo.identifier;\n    this.pricingPlan = managerInfo.pricingPlan;\n  },\n  methods: {\n    toggleMenu(menu) {\n      if (menu === \"operation\") {\n        this.isOperationMenuOpen = !this.isOperationMenuOpen;\n        this.isCustomerMenuOpen = false;\n        this.isStatisticsMenuOpen = false;\n        this.isContestMenuOpen = false;\n      } else if (menu === \"customer\") {\n        this.isCustomerMenuOpen = !this.isCustomerMenuOpen;\n        this.isOperationMenuOpen = false;\n        this.isStatisticsMenuOpen = false;\n        this.isContestMenuOpen = false;\n      } else if (menu === \"statistics\") {\n        this.isStatisticsMenuOpen = !this.isStatisticsMenuOpen;\n        this.isOperationMenuOpen = false;\n        this.isCustomerMenuOpen = false;\n        this.isContestMenuOpen = false;\n      } else if (menu === \"contest\") {\n        this.isContestMenuOpen = !this.isContestMenuOpen;\n        this.isOperationMenuOpen = false;\n        this.isCustomerMenuOpen = false;\n        this.isStatisticsMenuOpen = false;\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.sidebar {\n  width: 360px;\n  background-color: #fff;\n  padding: 20px;\n  border-right: 1px solid #ddd;\n  box-shadow: 2px 0 5px rgba(0, 0, 0, 0.1); /* Box shadow 추가 */\n}\n\n.store-info {\n  text-align: center;\n  margin-bottom: 20px;\n}\n\n.store-name {\n  margin-bottom: 10px;\n}\n\n.store-manager {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  gap: 10px;\n  margin-bottom: 10px;\n}\n\n.store-type {\n  background-color: #1e4eb0;\n  color: #fff;\n  padding: 10px 12px;\n  border-radius: 5px;\n}\n\n.contract-info {\n  border: 1px solid #ddd;\n}\n\n.contract-item {\n  display: flex;\n  justify-content: space-between;\n  padding: 10px 20px;\n}\n\n.contract-item span {\n  line-h